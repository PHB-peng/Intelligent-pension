<view class="pd-20 goods-list data-v-0f00adf4"><block a:if="{{empty==true}}"><view class="empty data-v-0f00adf4"><image src="/static/img/emptyCart.jpg" mode="aspectFit" class="data-v-0f00adf4"></image><view class="empty-tips data-v-0f00adf4">空空如也<navigator class="navigator data-v-0f00adf4" url="../index/index" open-type="switchTab">随便逛逛></navigator></view></view></block><view class=" pd-20 row data-v-0f00adf4" a:for="{{goodsList}}" a:for-item="list" a:for-index="index" a:key="index"><view class="top-firm-info data-v-0f00adf4"><view class="data-v-0f00adf4"><checkbox class="checkbox data-v-0f00adf4" color="#8a17c6" checked="{{list.selectedAll}}" data-event-opts="{{[['tap',[['firmSelectedAll',[index]]]]]}}" onTap="__e"></checkbox></view><view class="firm-box data-v-0f00adf4"><text class="firm-name data-v-0f00adf4">{{list.firmName}}</text></view></view><view class="firm-goods-list data-v-0f00adf4" a:for="{{list.goods}}" a:for-item="goods" a:for-index="i" a:key="i"><text data-event-opts="{{[['tap',[['deleteDatas',[index,i]]]]]}}" class="deleteData del-btn yticon icon-fork data-v-0f00adf4" onTap="__e">X</text><view class="data-v-0f00adf4"><checkbox class="checkbox data-v-0f00adf4" color="#8a17c6" checked="{{goods.selected}}" data-event-opts="{{[['tap',[['selectedSole',[index,i]]]]]}}" onTap="__e"></checkbox></view><image class="goods-img data-v-0f00adf4" src="{{goods.image}}"></image><view class="right-goods-box data-v-0f00adf4"><view class="goods-name data-v-0f00adf4">{{goods.title}}</view><view class=" goods-spec data-v-0f00adf4"><text class="text data-v-0f00adf4">{{goods.attr_val}}</text></view><view class="price-number-box data-v-0f00adf4"><view class="red-price data-v-0f00adf4">{{"￥"+goods.price}}</view></view><view class="number-box data-v-0f00adf4"><text data-event-opts="{{[['tap',[['sub',[index,i]]]]]}}" class="number-sub data-v-0f00adf4" onTap="__e">-</text><text class="number-num data-v-0f00adf4">{{goods.number}}</text><text data-event-opts="{{[['tap',[['add',[index,i]]]]]}}" class="number-add data-v-0f00adf4" onTap="__e">+</text></view></view></view></view><view class=" footer data-v-0f00adf4"><view class="check-box data-v-0f00adf4"><block a:if="{{goodsList.length!=0}}"><checkbox color="#8a17c6" checked="{{isSelectedAllRow}}" data-event-opts="{{[['tap',[['selectedAllRow',['$event']]]]]}}" onTap="__e" class="data-v-0f00adf4"></checkbox></block><block a:if="{{selectedAllRowLength}}"><view data-event-opts="{{[['tap',[['removeGoodsEvent',['$event']]]]]}}" class="goods-remove data-v-0f00adf4" onTap="__e">删除</view></block></view><view class="right-box data-v-0f00adf4"><block a:if="{{sumTotalPrices>0}}"><text class="total-cost data-v-0f00adf4">总价格<text class="red-price data-v-0f00adf4">{{"￥"+sumTotalPrices}}</text></text></block><text data-event-opts="{{[['tap',[['createOrder']]]]}}" class="account data-v-0f00adf4" onTap="__e">结算<block a:if="{{selectedAllRowLength}}"><text class="data-v-0f00adf4">{{"("+selectedAllRowLength+")"}}</text></block></text></view></view></view>